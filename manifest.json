{
  "name": "Boilerplate chrome extension",
  "description": "boilerplate of chrome extension",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./logo_16x16.png",
    "32": "./logo_32x32.png",
    "48": "./logo_48x48.png",
    "128": "./logo_128x128.png"
  },
  "background": {
    "scripts": [
      "./background.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "\u003Call_urls>",
    "storage"
  ]
}

